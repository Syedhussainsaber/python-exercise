number=int(input("enter The nunber:\n"))
num=0
if(number==1):
    print("The number is neither prime nor composite number\n")
if(number==2):
    print("The number is the prime number\n")
for x in range(2,number):
    if(number%x==0):
        print("The number is not a prime number")
        break
    else:
        print("The number is a prime number")
        break
